{
    "name": "fsasvari/laravel-uploadify",
    "description": "Library for Laravel that handles image uploading with renaming, showing thumbnail image with custom route and more.",
    "keywords": ["upload", "image", "file", "laravel", "php"],
    "license": "MIT",
    "homepage": "https://github.com/fsasvari/laravel-uploadify",
    "authors": [
        {
            "name": "Frano Sasvari",
            "email": "sasvari.frano@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*",
        "illuminate/http": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "intervention/image": "2.4.*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Uploadify\\": "src/Uploadify/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uploadify\\Providers\\UploadifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
